{
	"slider-layout#home-banner": {
		"props": {
			"itemsPerPage": {
				"desktop": 1,
				"tablet": 1,
				"phone": 1
			},
			"infinite": true,
			"showNavigationArrows": "desktopOnly",
			"showPaginationDots": "never",
			"blockClass": "carousel",
			"navigationStep": 1,
			"fullWidth": true,
			"autoplay": {
				"timeout": 8000,
				"stopOnHover": true
			},
			"slideTransition": { "speed": 1500, "delay": 0, "timing": "" }
